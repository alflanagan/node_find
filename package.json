{
  "name": "node_find",
  "version": "0.0.1",
  "description": "emulating some functions of UNIX find",
  "preferGlobal": true,
  "bin": "./build/node_find.js",
  "dependencies": {
    "@babel/polyfill": "^7.2.5",
    "micromatch": "^3.1.10",
    "yargs": "^6.4.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.2.2",
    "@babel/preset-env": "^7.3.1",
    "ink-docstrap": ">=1.3",
    "jasmine": "^3.3",
    "jsdoc": ">=3.4",
    "snyk": "^1.100.1",
    "standard": "^12.0.1"
  },
  "scripts": {
    "test": "npm run build && standard && jasmine",
    "makedoc": "node_modules/.bin/jsdoc src -r -c conf.json -R README.md -t ./node_modules/ink-docstrap/template -d docs",
    "build": "node_modules/.bin/babel src -d build && node_modules/.bin/babel spec -d build/spec"
  },
  "author": "A. Lloyd Flanagan",
  "license": "GPL-3.0",
  "repository": "alflanagan/node_find",
  "private": true,
  "engines": {
    "node": ">=8.0"
  },
  "keywords": [
    "find",
    "utility",
    "command-line"
  ]
}
