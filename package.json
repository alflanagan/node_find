{
  "name": "node_find",
  "version": "0.0.1",
  "description": "emulating some functions of UNIX find",
  "preferGlobal": true,
  "bin": {
    "nodefind": "node_find.js"
  },
  "dependencies": {
    "babel-polyfill": "^6.5.0",
    "yargs": "^3.32.0"
  },
  "devDependencies": {
    "babel-cli": "^6.3.17",
    "babel-core": "^6.5.2",
    "babel-plugin-syntax-async-functions": "^6.3.13",
    "babel-preset-es2015": "^6.3.13",
    "ink-docstrap": "^1.1.2",
    "jasmine": "^2.4.1",
    "jsdoc": "^3.4.0"
  },
  "scripts": {
    "test": "npm run-script build > /dev/null && jasmine",
    "build": "node_modules/.bin/babel src -d build; node_modules/.bin/babel spec/es6 -d spec",
    "prepublish": "npm run-script build",
    "makedoc": "node_modules/.bin/jsdoc src -r -c conf.json -R README.md -t ./node_modules/ink-docstrap/template -d docs"
  },
  "author": "A. Lloyd Flanagan",
  "license": "GPL-3.0",
  "repository": "alflanagan/node_find",
  "private": true,
  "engines": {
    "node": ">=5.3"
  },
  "babel": {
    "babelrc": false,
    "presets": [
      "es2015"
    ]
  },
  "keywords": [
    "find",
    "utility",
    "command-line"
  ]
}
